%h1
  = I18n.t('events.title')
%table
  %thead
    %th
      IP
    %th
      = I18n.t('events.longitude')
    %th
      = I18n.t('events.latitude')
    %th
      = I18n.t('events.timezone')
  %tbody
  - @events.each do |e|
    %tr
      %td= e.ip
      %td= e.longitude
      %td= e.latitude
      %td= e.created_at.strftime('%Y-%m-%d %H:%M:%S')
%hr
%ul
  %li
    = link_to I18n.t('navigation.home'), :root
