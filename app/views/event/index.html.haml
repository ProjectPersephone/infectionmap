%table
  %thead
    %th
      = I18n.t('events.disease')
    %th
      = I18n.t('events.longitude')
    %th
      = I18n.t('events.latitude')
    %th
      = I18n.t('events.timezone')
  %tbody
  - @events.each do |e|
    %tr
      %td= e.event_type
      %td= e.longitude
      %td= e.latitude
      %td= e.created_at.strftime('%Y-%m-%d %H:%M:%S')

